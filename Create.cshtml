@model GimnasioMVC.Dto.SocioDto
@{
    ViewData["Title"] = "Crear Socio";
}

<h2>Crear Socio</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label for="Nombre">Nombre</label>
        <input type="text" class="form-control" asp-for="Nombre" />
    </div>
    <div class="form-group">
        <label for="Apellido">Apellido</label>
        <input type="text" class="form-control" asp-for="Apellido" />
    </div>
    <div class="form-group">
        <label for="Telefono">Telefono</label>
        <input type="tel" class="form-control" asp-for="NumeroDeTelefono" />
    </div>
    <button type="submit" class="btn btn-success">Guardar</button>
</form>
